<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:symbolsViewModel="clr-namespace:SymbolsViewModel;assembly=SymbolsViewModel"
             mc:Ignorable="d" 
             d:DesignWidth="800" 
             d:DesignHeight="450"
             x:Class="SymbolsView.ActionSymbolView"
             x:DataType="symbolsViewModel:ActionSymbolViewModel">
    
    <Design.DataContext>
        <symbolsViewModel:ActionSymbolViewModel />
    </Design.DataContext>
    
    <Canvas
        HorizontalAlignment="Left"
        VerticalAlignment="Center"
        Width="{Binding Width}"
        Height="{Binding Height}"
        Background="#FF52C0AA">
    </Canvas>
</UserControl>